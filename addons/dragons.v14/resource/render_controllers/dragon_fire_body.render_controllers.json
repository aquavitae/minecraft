          "Array.normal": [ "Texture.dragon_fire1", "Texture.dragon_fire2", "Texture.dragon_fire3" ],
          "Array.saddled": [ "Texture.dragon_fire1_saddled", "Texture.dragon_fire2_saddled", "Texture.dragon_fire3_saddled" ]
